    <view id="Tutorial1">
        <background id="background"
        color="#5d3da0"
        />

        <video id="tip_video" value="01_tut_contraste.mp4"/>

        <imageBackground id="imageBackground"
        image="background.jpg"
        scale="ASPECT_FILL"
        circle_color="#ff336680"
        />

