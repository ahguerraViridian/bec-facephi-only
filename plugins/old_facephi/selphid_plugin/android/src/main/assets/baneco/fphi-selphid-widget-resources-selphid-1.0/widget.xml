        font_size="16.0"
        align="LEFT"
        />
        
        <button id="buttonNext"
        background="#00000000"
        foreground="#ffffff"
        content="arrow-next-light.png"
        content_type="RESOURCE_ID"
        align="RIGHT"
        />        

