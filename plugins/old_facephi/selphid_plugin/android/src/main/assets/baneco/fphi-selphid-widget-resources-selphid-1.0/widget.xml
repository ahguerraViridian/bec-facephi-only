<?xml version="1.0" encoding="UTF-8"?>
<facephi-widget-conf version="1.2"
    border_width="4.0"
    border_color_tip="#ff3366"
    background_color_tip="#ff33664c"
    border_color_capture="#ffffff"
    border_color_success="#ffffff"
    background_color_success="#33ff924c"
    border_color_error="#ffffff"
    background_color_error="#ff763933"
    >

    <view id="CaptureIDFront">

        <gradientBackground id="background"
        colorStart="#1c113580"
        colorEnd="#c0c0c080"
        xStart="0.333" yStart="0.200"
        xEnd="0.750" yEnd="1.166"
        />

        <panel id="IDPanel"
        color="#ff33664c"
        />

        <frameID id="frame"
        color="#ed1c24"
        />

        <labeledIcon id="tipCaptureFront"
        icon="IDIconFront.png"
        icon_align="LEFT"
        separation="16.0"
        text="CaptureTipFront"
        text_color="#f9fafc"
        text_align="LEFT"
        font="d-din-regular.ttf"
        font_size="20.0"
        line_height="26.0"
        />


        <gradientPanel id="infoPanel"
        colorStart="#ed1c24"
        colorEnd="#ed1c24"
        xStart="0.333" yStart="0.200"
        xEnd="0.750" yEnd="1.166"
        />

        <labeledIcon id="infoCaptureFront"
        icon="IDIconSMFront.png"
        icon_align="LEFT"
        separation="16.0"
        text="CaptureInfoFront"
        text_color="#f9fafc"
        text_align="LEFT"
        font="d-din-regular.ttf"
        font_size="20.0"
        line_height="22.0"
        />

        <!-- <buttonImage id="buttonInfo" value="info-light.png"/> -->
        <buttonImage id="buttonExit" value="cross-light.png"/>

        <image id="logo" value="logoSelphIDLight.png"/>
    </view>


